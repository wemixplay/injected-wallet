{
  "name": "@wemixplay/injected-wallet",
  "version": "0.9.2",
  "description": "Wemix Play Wallet with Injected Wallet",
  "keywords": [
    "Ethereum",
    "Web3",
    "EVM",
    "dapp",
    "Multichain",
    "Wallet",
    "Transaction",
    "Provider",
    "Injected Wallet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wemixplay/injected-wallet.git",
    "directory": "packages/playWallet"
  },
  "homepage": "https://wemixplay.com",
  "bugs": "https://github.com/wemixplay/injected-wallet/issues",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "main": "dist/index.js",
  "type": "module",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "copy-files": "cp ./src/sdk/wemix.js ./dist/sdk/wemix.js",
    "build": "yarn clean && tsc && yarn copy-files",
    "dev": "tsc -w",
    "type-check": "tsc --noEmit",
    "lint": "yarn lint-eslint && yarn lint-prettier",
    "lint-eslint": "eslint --ignore-path .gitignore \"src/**/*{ts,tsx}\" --fix",
    "lint-prettier": "prettier --write \"src/**/*.(ts|tsx)\"",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^20.11.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.1.0",
    "tslib": "^2.6.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@web3-onboard/common": "^2.3.3",
    "@wemixplay/injected-wallet": "file:../../Github/injected-wallet",
    "axios": "^1.6.3",
    "ethers": "^5.7.2",
    "eventemitter3": "^5.0.1",
    "zustand": "^4.4.7"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.js": "eslint --cache --fix"
  }
}